{
    "version": 2,
    "builds": [
        {
            "src": "frontend/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
{
                "version": 2,
                "builds": [
                    {
                        "src": "frontend/package.json",
                        "use": "@vercel/static-build",
                        "config": {
                            "distDir": "dist"
                        }
                    },
                    {
                        "src": "app/main.py",
                        "use": "@vercel/python"
                    }
                ],
                "routes": [
                    {
                        "src": "/api/(.*)",
                        "dest": "app/main.py"
                    },
                    {
                        "src": "/(.*)",
                        "dest": "frontend/$1"
                    }
                ],
                "build": {
                    "env": {
                        "NODE_VERSION": "18"
                    }
                }
            }